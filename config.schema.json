{
  "pluginAlias": "ArcamAVR",
  "pluginType": "platform",
  "singular": true,
  "schema": {
    "type": "object",
    "required": ["model", "ipAddress"],
    "properties": {
      "model": {
        "title": "Model",
        "type": "string",
        "required": true,
        "default": "AVR11",
        "oneOf": []
      },
      "ipAddress": {
          "title": "IP Address",
          "type": "string",
          "required": true
      },
      "port": {
          "title": "Port",
          "type": "integer",
          "required": false,
          "default": "50000"
       }
    }
  }
}
